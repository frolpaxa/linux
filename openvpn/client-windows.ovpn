client
#dev [tap|tun] # для использования openvpn в качестве шлюза
#dev-node {OVPN} # для использования openvpn в качестве шлюза
dev tun
proto tcp
remote xxx.xxx.xxx.xxx 1194 # где xxx.xxx.xxx.xxx - ip сервера openvpn
resolv-retry infinite
nobind
persist-key
persist-tun
ca c:\\vpn\\ca.crt
cert c:\\vpn\\client.crt
key c:\\vpn\\client.key
comp-lzo
verb 3
